public class App 
{
	public static void main(String[] args) 
	{
		Person p1,p2,p3;
		p1=new Person(102,"Rahul","Noida");
		p2=new Person(103,"Hemant","Delhi");
		p3=new Person(101,"imran","Lucknow");
		
		p1=p2;
		p2=p3;
		p3=p1;
		p1.showDetails();//?103
		p2.showDetails();//?101
		p3.showDetails();//?103
	}
}
